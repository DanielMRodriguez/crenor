---
import Base from "@/layouts/Base.astro";
import { getEntryBySlug } from "astro:content";
import PageHeader from "@/layouts/components/PageHeader.astro";
import HowWorks from "@/layouts/components/sections/HowWorks.astro";
import WhyCrenor from "@/layouts/components/sections/WhyCrenor.astro";
import Testimonials from "@/components/sections/Testimonials.astro";
import PreguntasFrecuentes from "@/layouts/components/sections/PreguntasFrecuentes.astro";
import Impulsamos from "@/layouts/components/sections/Impulsamos.astro";
import Direccion from "@/layouts/components/sections/Direccion.astro";
import MisionVision from "@/layouts/components/sections/MisionVision.astro";
const homepage = await getEntryBySlug("homepage", "index");
const { banner, key_features, service, testimonial } = homepage.data;
---

<Base>
  <PageHeader />
  <HowWorks />
  <WhyCrenor />
  <Testimonials />
  <PreguntasFrecuentes />
  <Impulsamos />
  <MisionVision />
  <Direccion />
</Base>
